    "nodemailer-sendgrid-transport": "^0.2.0",
    "paypal-node-sdk": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "socket.io": "^2.3.0",
    "stripe": "^18.1.0",
    "twilio": "^3.46.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
